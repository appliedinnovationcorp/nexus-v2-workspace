{
  "name": "@aic/analytics-engine",
  "version": "1.0.0",
  "description": "Enterprise-grade Real-Time Analytics Engine for AIC Platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:client",
    "build:client": "webpack --mode production",
    "dev": "concurrently \"tsc --watch\" \"nodemon dist/server.js\"",
    "start": "node dist/server.js",
    "start:analytics": "node dist/analytics-engine.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "jest --config jest.performance.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "node dist/database/migrate.js",
    "db:seed": "node dist/database/seed.js",
    "docker:build": "docker build -t aic-analytics-engine .",
    "docker:run": "docker run -p 3000:3000 aic-analytics-engine",
    "docs": "typedoc src --out docs",
    "benchmark": "node dist/benchmarks/performance.js"
  },
  "keywords": [
    "analytics",
    "real-time",
    "business-intelligence",
    "ai-insights",
    "metrics",
    "dashboard",
    "enterprise"
  ],
  "author": "AIC Development Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.0.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "redis": "^4.6.7",
    "mongodb": "^5.7.0",
    "influxdb-client": "^1.33.2",
    "kafka-node": "^5.0.0",
    "bull": "^4.11.3",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^2.4.2",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "axios": "^1.4.0",
    "ws": "^8.13.0",
    "graphql": "^16.7.1",
    "apollo-server-express": "^3.12.0",
    "dataloader": "^2.2.2",
    "openai": "^4.0.0",
    "anthropic": "^0.20.0",
    "prometheus-client": "^14.2.0",
    "elastic-apm-node": "^4.0.0",
    "node-cron": "^3.0.2",
    "csv-parser": "^3.0.0",
    "fast-csv": "^4.3.6",
    "sharp": "^0.32.1",
    "puppeteer": "^20.7.2",
    "nodemailer": "^6.9.4",
    "twilio": "^4.14.0",
    "slack-web-api": "^6.8.1"
  },
  "devDependencies": {
    "typescript": "^5.1.6",
    "ts-node": "^10.9.1",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "prettier": "^3.0.0",
    "typedoc": "^0.24.8",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "ts-loader": "^9.4.4",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "css-loader": "^6.8.1",
    "sass-loader": "^13.3.2",
    "sass": "^1.64.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aic-platform/analytics-engine.git"
  },
  "bugs": {
    "url": "https://github.com/aic-platform/analytics-engine/issues"
  },
  "homepage": "https://github.com/aic-platform/analytics-engine#readme"
}
