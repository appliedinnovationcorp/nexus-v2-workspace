# Analytics Engine Environment Variables

# Server Configuration
PORT=3001
HOST=0.0.0.0
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/analytics
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your-influxdb-token
INFLUXDB_ORG=aic
INFLUXDB_BUCKET=analytics

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=0

# Kafka Configuration (Optional)
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=analytics-engine
KAFKA_GROUP_ID=analytics-group

# AI Configuration
AI_ENABLED=true
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4
ANTHROPIC_API_KEY=your-anthropic-api-key
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h
BCRYPT_ROUNDS=12
API_KEYS=key1,key2,key3

# Monitoring Configuration
PROMETHEUS_ENABLED=true
PROMETHEUS_PORT=9090
LOG_LEVEL=info
LOG_FORMAT=json
APM_ENABLED=false
APM_SERVICE_NAME=analytics-engine

# CORS Configuration
CORS_ORIGIN=http://localhost:3000,https://aic.com
CORS_CREDENTIALS=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=1000

# Feature Flags
ENABLE_REAL_TIME_PROCESSING=true
ENABLE_AI_INSIGHTS=true
ENABLE_ANOMALY_DETECTION=true
ENABLE_PREDICTIVE_ANALYTICS=true
ENABLE_DASHBOARD_SHARING=true
ENABLE_ALERT_NOTIFICATIONS=true

# Performance Tuning
MAX_EVENT_BUFFER_SIZE=10000
EVENT_FLUSH_INTERVAL=5000
METRICS_CALCULATION_INTERVAL=60000
INSIGHTS_GENERATION_INTERVAL=300000

# Data Retention
EVENT_RETENTION_DAYS=90
METRICS_RETENTION_DAYS=365
INSIGHTS_RETENTION_DAYS=180
LOGS_RETENTION_DAYS=30
